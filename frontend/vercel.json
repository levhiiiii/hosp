{
  "version": 2,
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://hosp-245y.onrender.com/api/$1"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*\\.(js|css|png|jpg|jpeg|gif|svg|ico|woff|woff2|ttf|eot))",
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "VITE_API_BASE_URL": "https://hosp-245y.onrender.com/api"
  }
}
